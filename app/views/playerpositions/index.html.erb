<p id="notice"><%= notice %></p>

<h1>Listing Playerpositions</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @playerpositions.each do |playerposition| %>
      <tr>
        <td><%= playerposition.description %></td>
        <td><%= link_to 'Show', playerposition %></td>
        <td><%= link_to 'Edit', edit_playerposition_path(playerposition) %></td>
        <td><%= link_to 'Destroy', playerposition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Playerposition', new_playerposition_path %>
